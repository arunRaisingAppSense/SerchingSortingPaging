<p id="notice"><%= notice %></p>

<h1>Clints</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clints.each do |clint| %>
      <tr>
        <td><%= link_to 'Show', clint %></td>
        <td><%= link_to 'Edit', edit_clint_path(clint) %></td>
        <td><%= link_to 'Destroy', clint, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Clint', new_clint_path %>
