 <% if @searchProjects.present?%>
 <table>
  <thead>
    <tr>
      <th><%= sort_link "id" , "Project ID" %></th>
      <th><%= sort_link "name" , "Name" %></th>
      <th><%= sort_link "duration" , "Duration" %></th>
      <th><%= sort_link "created_at" , "Created At" %></th>
      <th><%= sort_link "updated_at" , "Updated At" %></th>
      <th colspan="3"></th>
    </tr>
  </thead>
 <tbody class="project_tbody_content">
    <% @searchProjects.each do |project| %>
      <%# byebug %>
      <tr>
        <td><%= project.id %></td>
        <td><%= project.name %></td>
        <td><%= project.duration %></td>
        <td><%= project.created_at.strftime("Date(%d-%B-%y) Time(%T)") %></td>
        <td><%= project.updated_at.strftime("Date(%d-%m-%Y) Time(%T)") %></td>
        <td><%= link_to 'Show', project %></td>
        <td><%= link_to 'Edit', edit_project_path(project) %></td>
        <td><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody></table>
<%= will_paginate @projects %> 
  <% else %>
    <p> No records found</p>
  <% end %>
  